<div id="contact-form" class="col-xs-12 col-md-12">
  <h4 class="text-left">{$h4}</h4>
  {*control kontaktForm*} 
  <form n:name=kontaktForm>
		<div class="form-group">
			<label for="username" n:name=meno>Va코e meno: </label>
      <input name="username" n:name=meno type="text" class="form-control">
		</div>
		<div>
  		<label for="email" n:name=email>V치코 email: </label>
      <input name="email" n:name=email type="email" class="form-control">
  	</div>
		<div>
  		<label for="message" n:name=text>Spr치va: </label>
      <textarea name="message" n:name=text class="form-control" rows="4"></textarea>
  	</div>
		<div>
  	<label></label><input n:name=uloz type="submit" class="form-control button">
		</div>
  </form>
</div> 